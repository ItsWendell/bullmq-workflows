{
  "name": "bullmq-workflows",
  "version": "0.1.0-experimental.0",
  "description": "A TypeScript durable workflow engine built on BullMQ and Redis with deterministic replay, nested steps, retries, and event-driven patterns. Inspired by Cloudflare Workflows.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "bullmq",
    "workflow",
    "workflow-engine",
    "durable-execution",
    "queue",
    "jobs",
    "redis",
    "typescript",
    "async",
    "task-scheduling",
    "deterministic-replay",
    "orchestration",
    "saga",
    "retries",
    "event-driven",
    "cloudflare-workflows"
  ],
  "author": "BullMQ Workflows Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bun-workflows.ts.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/bun-workflows.ts/issues"
  },
  "homepage": "https://github.com/yourusername/bun-workflows.ts#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "ioredis": "^5.8.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "ioredis": "^5",
    "typescript": "^5"
  },
  "dependencies": {
    "bullmq": "^5.61.0",
    "capnweb": "^0.1.0"
  }
}
